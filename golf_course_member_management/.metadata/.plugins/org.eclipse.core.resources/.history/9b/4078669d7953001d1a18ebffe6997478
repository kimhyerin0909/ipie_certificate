const user_name = document.getElementById("user_name");
const user_id = document.getElementById("user_id");
const place = document.getElementById("place");
const class_name = document.getElementById("class_name");
const class_cost = document.getElementById("class_cost");

function handleUserInfo() {
	if(user_name.value === "1") {
		user_id.value = "10001";
	} else if(user_name.value === "2") {
		user_id.value = "10002";
	} else if(user_name.value === "3") {
		user_id.value ="10003";
	} else if(user_name.value === "4") {
		user_id.value = "20001";
	} else if(user_name.value === "5") {
		user_id.value = "20001";
	}
}

