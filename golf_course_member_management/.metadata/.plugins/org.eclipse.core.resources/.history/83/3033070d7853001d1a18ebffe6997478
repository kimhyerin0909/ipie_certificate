const user_name = document.getElementById("user_name");
const user_id = document.getElementById("user_id");

function handleUserInfo() {
	
	alert(user_name.value)
	switch(user_name.value) {
		case 1:
			user_id = "10001";
			break;
		case 2:
			user_id = "10002";
			break;
		case 3:
			user_id = "10003";
			break;
		case 4:
			user_id = "20001";
			break;
		case 5:
			user_id = "20002";
			break;
	}
}